{
  "name": "xml",
  "description": "XML components for ChiÏ‡",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodule/xml.git"
  },
  "dependencies": {
    "xml2json": "0.x.x"
  },
  "nodes": [
    {
      "name": "2json",
      "ns": "xml",
      "title": "XML To JSON",
      "description": "Converts XML to JSON",
      "phrases": {
        "active": "Converting XML to JSON"
      },
      "ports": {
        "input": {
          "in": {
            "title": "XML",
            "type": "string",
            "required": true
          }
        },
        "output": {
          "out": {
            "title": "Object",
            "type": "object"
          }
        }
      },
      "dependencies": {
        "npm": {
          "xml2json": "0.x.x"
        }
      },
      "env": "server",
      "fn": "output.out = $.write('in', xml2json.toJson($.in, {object: true}))\n"
    }
  ],
  "env": "server"
}